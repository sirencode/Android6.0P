<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CALENDAR"
        android:id="@+id/btn_calendar"
        android:layout_marginLeft="74dp"
        android:layout_marginStart="74dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CAMERA"
        android:id="@+id/btn_camera"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/btn_calendar"
        android:layout_toEndOf="@+id/btn_calendar" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Contacts"
        android:id="@+id/btn_Contacts"
        android:layout_below="@+id/btn_calendar"
        android:layout_toLeftOf="@+id/btn_camera"
        android:layout_toStartOf="@+id/btn_camera" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Location"
        android:id="@+id/btn_Location"
        android:layout_below="@+id/btn_calendar"
        android:layout_toRightOf="@+id/btn_calendar"
        android:layout_toEndOf="@+id/btn_calendar" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MicroPhone"
        android:id="@+id/btn_MicroPhone"
        android:layout_below="@+id/btn_Contacts"
        android:layout_toLeftOf="@+id/btn_Location"
        android:layout_toStartOf="@+id/btn_Location" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Phone"
        android:id="@+id/btn_phone"
        android:layout_below="@+id/btn_Contacts"
        android:layout_toRightOf="@+id/btn_Contacts"
        android:layout_toEndOf="@+id/btn_Contacts" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sensors"
        android:id="@+id/btn_Sensors"
        android:layout_below="@+id/btn_MicroPhone"
        android:layout_toLeftOf="@+id/btn_sms"
        android:layout_toStartOf="@+id/btn_sms" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="SMS"
        android:id="@+id/btn_sms"
        android:layout_above="@+id/btn_Storage"
        android:layout_toRightOf="@+id/btn_MicroPhone"
        android:layout_toEndOf="@+id/btn_MicroPhone" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Storage"
        android:id="@+id/btn_Storage"
        android:layout_below="@+id/btn_Sensors"
        android:layout_alignLeft="@+id/btn_calendar"
        android:layout_alignStart="@+id/btn_calendar" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Write Settings"
        android:id="@+id/btn_writeSettings"
        android:layout_below="@+id/btn_Storage"
        android:layout_alignLeft="@+id/btn_Sensors"
        android:layout_alignStart="@+id/btn_Sensors"
        android:layout_marginTop="43dp"
        android:layout_alignRight="@+id/btn_Location"
        android:layout_alignEnd="@+id/btn_Location" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="System Alert Window"
        android:id="@+id/btn_saw"
        android:layout_below="@+id/btn_writeSettings"
        android:layout_alignLeft="@+id/btn_writeSettings"
        android:layout_alignStart="@+id/btn_writeSettings"
        android:layout_alignRight="@+id/btn_sms"
        android:layout_alignEnd="@+id/btn_sms" />
</RelativeLayout>
